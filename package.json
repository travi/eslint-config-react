{
  "name": "@travi/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/eslint-config-react",
  "bugs": "https://github.com/travi/eslint-config-react/issues",
  "homepage": "https://npm.im/@travi/eslint-config-react",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/eslint-config-react": "1.16.58"
  },
  "devDependencies": {
    "@travi/babel-preset": "3.0.136",
    "@travi/eslint-config": "5.0.11",
    "ban-sensitive-files": "1.10.7",
    "commitlint-config-travi": "1.4.54",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "npm-run-all2": "7.0.2",
    "publint": "0.3.2",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.6"
  }
}
