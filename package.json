{
  "name": "@travi/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "travi/eslint-config-react",
  "bugs": "https://github.com/travi/eslint-config-react/issues",
  "homepage": "https://npm.im/@travi/eslint-config-react",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@travi/eslint-config": "1.0.50",
    "ban-sensitive-files": "1.9.12",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@form8ion/eslint-config-react": "1.15.30"
  }
}
